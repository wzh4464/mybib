@article{xu2018PSAPPseudonymBasedSecure,
  title = {{{PSAP}}: {{Pseudonym-Based Secure Authentication Protocol}} for {{NFC Applications}}},
  shorttitle = {{{PSAP}}},
  author = {Xu, Jie and Xue, Kaiping and Yang, Qingyou and Hong, Peilin},
  year = {2018},
  journal = {IEEE Transactions on Consumer Electronics},
  volume = {64},
  number = {1},
  pages = {83--91},
  issn = {1558-4127},
  doi = {10.1109/TCE.2018.2811260},
  abstract = {Nowadays, near field communication (NFC) has been widely used in electronic payment, ticketing, and many other areas. NFC security standard requires the use of public key infrastructure (PKI) to implement mutual authentication and session keys negotiation in order to ensure communication security. In traditional PKI-based schemes, every user uses a fixed public/private key pair to implement authentication and key agreement. An attacker can create a profile based on user's public key to track and compromise the user's privacy. Recently, He et al. and Odelu et al. successively proposed pseudonym-based authentication key and agreement protocols for NFC after Eun et al.'s protocol (2013), which is first claimed to provide conditional privacy for NFC. They respectively claimed that their scheme can satisfy the security requirements. In this paper, first, we prove that their protocols still have security flaws, including the confusion of the user's identity and the random identity. Then, we propose a pseudonym-based secure authentication protocol (PSAP) for NFC applications, which is effective in lifetime and includes time synchronization-based method and nonce-based method. In our scheme, trusted service manager issues pseudonyms but does not need to maintain verification tables and it could reveal the user's identity of internal attackers. Furthermore, security and performance analysis proves that PSAP can provide traceability and more secure features with a little more cost.},
  langid = {english},
  keywords = {_tablet,Authentication,Authentication and key agreement,conditional privacy protection,Near field communication,near field communication (NFC),Privacy,Protocols,pseudonym,Public key,traceability}
}

@article{xu2019HealthchainBlockchainBasedPrivacy,
  title = {Healthchain: {{A Blockchain-Based Privacy Preserving Scheme}} for {{Large-Scale Health Data}}},
  shorttitle = {Healthchain},
  author = {Xu, Jie and Xue, Kaiping and Li, Shaohua and Tian, Hangyu and Hong, Jianan and Hong, Peilin and Yu, Nenghai},
  year = {2019},
  journal = {IEEE Internet of Things Journal},
  volume = {6},
  number = {5},
  pages = {8770--8781},
  issn = {2327-4662},
  doi = {10.1109/JIOT.2019.2923525},
  abstract = {With the dramatically increasing deployment of the Internet of Things (IoT), remote monitoring of health data to achieve intelligent healthcare has received great attention recently. However, due to the limited computing power and storage capacity of IoT devices, users' health data are generally stored in a centralized third party, such as the hospital database or cloud, and make users lose control of their health data, which can easily result in privacy leakage and single-point bottleneck. In this paper, we propose Healthchain, a large-scale health data privacy preserving scheme based on blockchain technology, where health data are encrypted to conduct fine-grained access control. Specifically, users can effectively revoke or add authorized doctors by leveraging user transactions for key management. Furthermore, by introducing Healthchain, both IoT data and doctor diagnosis cannot be deleted or tampered with so as to avoid medical disputes. Security analysis and experimental results show that the proposed Healthchain is applicable for smart healthcare system.},
  langid = {english},
  keywords = {_tablet,Access control,Blockchain,Cloud computing,Data privacy,dynamic key management,fine-grained access control,Internet of Things,Internet of Things (IoT),Medical services,privacy preserving,Servers,smart healthcare}
}

@article{xu2020IdentityManagementAuthentication,
  title = {An {{Identity Management}} and {{Authentication Scheme Based}} on {{Redactable Blockchain}} for {{Mobile Networks}}},
  author = {Xu, Jie and Xue, Kaiping and Tian, Hangyu and Hong, Jianan and Wei, David S. L. and Hong, Peilin},
  year = {2020},
  month = jun,
  journal = {IEEE Transactions on Vehicular Technology},
  volume = {69},
  number = {6},
  pages = {6688--6698},
  issn = {1939-9359},
  doi = {10.1109/TVT.2020.2986041},
  abstract = {More and more users are eager to obtain more comprehensive network services without revealing their private information. Traditionally, in order to access a network, a user is authorized with an identity and corresponding keys, which are generated and managed by the network operator. All users' personally identifying information are centralized stored by the network operator. However, this approach makes users lose the control of their personally identifying information. Users are concerned about who can access these sensitive data and whether they have been compromised. In this paper, we propose a blockchain-based identity management and authentication scheme for mobile networks, where users' identifying information are controlled by the users themselves. Our scheme let users generate their self-sovereign identities (SSIs) and corresponding public keys and private keys. The private key used to authenticate the user's identifying information is only known to the user. We use blockchain to record SSIs and public keys of legitimate user, and adopt chameleon hash to delete illegal users' information on the blockchain, while keeping the block head unchanged. Furthermore, other service providers can obtain the user's SSI and public key and authenticate users by querying the blockchain. Experimental results confirm that our scheme can greatly reduce the revocation overhead and communication overhead.},
  langid = {english},
  keywords = {_tablet,Authentication,chameleon hash,Communication system security,Identity management,mutual authentication,Public key,redactable blockchain,Servers,Wireless communication}
}

@inproceedings{xu2021OccamSecureAdaptive,
  title = {Occam: {{A Secure}} and {{Adaptive Scaling Scheme}} for {{Permissionless Blockchain}}},
  shorttitle = {Occam},
  booktitle = {2021 {{IEEE}} 41st {{International Conference}} on {{Distributed Computing Systems}} ({{ICDCS}})},
  author = {Xu, Jie and Cheng, Yingying and Wang, Cong and Jia, Xiaohua},
  year = {2021},
  month = jul,
  pages = {618--628},
  issn = {2575-8411},
  doi = {10.1109/ICDCS51616.2021.00065},
  abstract = {Blockchain scalability is one of the most desired properties for permissionless blockchain. Many recent blockchain protocols have focused on increasing the transaction throughput. However, existing protocols cannot dynamically scale the throughput to meet transaction demand. In this paper, we propose Occam, a secure and adaptive scaling scheme. Occam adaptively changes the transaction throughput by expanding and shrinking according to the transaction demand in the network. We introduce a dynamic adjustment mechanism of mining difficulty and a mining power load balancing mechanism to resist various attacks. Furthermore, we implement Occam on Amazon EC2 cluster with 1000 full nodes. Experimental results show that Occam can greatly increase the throughput of the blockchain and the mining power utilization.},
  langid = {english},
  keywords = {_tablet,Adaptive systems,Conferences,Directed Acyclic Graph (DAG),Distributed Consensus,Load management,Proof of Work (PoW),Protocols,Resists,Scalability,Throughput}
}

@unpublished{xuAdaptiveScalingBlockchain,
  title = {An {{Adaptive Scaling Blockchain Protocol}} with {{Deduplication}} of {{Transactions}}},
  author = {XU, Jie and XIE, Qingyuan and PENG, Sen and WANG, Cong and Jia, Xiaohong},
  abstract = {With the development of cryptocurrencies, there is an urgent need for high-performance blockchains. Although existing schemes improve blockchain throughput by allowing multiple miners to append blocks to the blockchain concurrently, little attention has been devoted to how to adjust blockchain throughput dynamically and how to avoid transaction duplication between concurrent blocks. This paper presents an adaptive scaling blockchain protocol with deduplication of transactions. When the transaction demand in the network is high, the blockchain expands to meet users' demand; when the transaction demand is low, the blockchain shrinks to save communication and storage costs. We design a transaction deduplication mechanism to ensure that no duplicate transactions are appended to the blockchain to improve bandwidth utilization and achieve higher effective throughput. Besides, the mining power load balancing mechanism can ensure the security of the blockchain by splitting adversaries' mining power. The proposed prototype has been implemented on Amazon EC2. The experiment results show that the proposed protocol achieves the dynamic throughput and reach xxxtpstransactions per second.},
  langid = {english},
  keywords = {_tablet}
}

