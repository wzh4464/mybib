@article{yang2019AnFRAAnonymousFast,
  title = {Anfra: {{Anonymous}} and {{Fast Roaming Authentication}} for {{Space Information Network}}},
  shorttitle = {{{AnFRA}}},
  author = {Yang, Qingyou and Xue, Kaiping and Xu, Jie and Wang, Jiajie and Li, Fenghua and Yu, Nenghai},
  year = {2019},
  journal = {IEEE Transactions on Information Forensics and Security},
  volume = {14},
  number = {2},
  pages = {486--497},
  issn = {1556-6021},
  doi = {10.1109/TIFS.2018.2854740},
  abstract = {Nowadays, the Space Information Network (SIN) has been widely used in real life because of its advantages of communicating anywhere at any time. This feature is leading to a new trend that traditional wireless users are willing to roam to SIN to obtain a better service. However, the features of exposed links and higher signal latency in SIN make it difficult to design a secure and fast roaming authentication scheme for this new trend. Although some existing researches have been focused on designing secure authentication protocols for SIN or providing roaming authentication protocols for traditional wireless networks, these schemes cannot provide adequate requirements for the roaming communication in SIN and bring in critical issues, such as the privacy leakage or intolerable authentication delay. Observing these problems have not been well addressed, we design an anonymous and fast roaming authentication scheme for SIN. In our scheme, we utilize the group signature to provide the anonymity for roaming users, and assume that the satellites have limited computing capacity and make them have the defined authentication function to avoid the real-time involvement of the home network control center when authenticating the roaming users. The results of security and performance analysis show that the proposed scheme can provide the required security features, while providing a small authentication delay.},
  langid = {english}
}

@inproceedings{yang2022FastImageProcess,
  title = {Fast {{Image Process}} and {{Detection}} of {{Distorted Blur-Readable}} 2d {{Barcode}}},
  booktitle = {2022 7th International Conference on Signal and Image Processing ({{ICSIP}})},
  author = {Yang, Yuting and Lei, Changfa and Chen, Rongjun and Huang, Hao and Tan, Hong-Zhou},
  year = {2022},
  month = jul,
  pages = {322--326},
  doi = {10.1109/ICSIP55141.2022.9887142},
  abstract = {Blurring of two-dimensional (2D) barcode is very common in mobile Internet systems, which often leads to barcode recognition failure. In order to deal with the problem of barcode blur, blur-readable (BR) 2D barcode which can be decoded without deblurring is newly proposed. However, the research on the image processing of BR code are rare and the field of BR code detection and identification is almost blank so far, which impede the use of BR code in practical scenarios. In the application of BR code, distortion problems such as tilt and perspective of the image may lead to difficulty in identifying the BR code. In addition, in some industrial scenarios, we hope that the BR code can be instantly recognized to meet the real-time requirement. In this paper, an efficient image processing and detection method for BR code images with complex background is proposed. We use the ellipse feature of the finder patterns of distorted BR code to locate and correct the BR code images. The experimental results show that our BR code detection method is effective to detect distorted BR code images.},
  langid = {english}
}

