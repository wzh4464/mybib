@unpublished{xuAdaptiveScalingBlockchain,
  title = {An {{Adaptive Scaling Blockchain Protocol}} with {{Deduplication}} of {{Transactions}}},
  author = {XU, Jie and XIE, Qingyuan and PENG, Sen and WANG, Cong and Jia, Xiaohong},
  abstract = {With the development of cryptocurrencies, there is an urgent need for high-performance blockchains. Although existing schemes improve blockchain throughput by allowing multiple miners to append blocks to the blockchain concurrently, little attention has been devoted to how to adjust blockchain throughput dynamically and how to avoid transaction duplication between concurrent blocks. This paper presents an adaptive scaling blockchain protocol with deduplication of transactions. When the transaction demand in the network is high, the blockchain expands to meet users' demand; when the transaction demand is low, the blockchain shrinks to save communication and storage costs. We design a transaction deduplication mechanism to ensure that no duplicate transactions are appended to the blockchain to improve bandwidth utilization and achieve higher effective throughput. Besides, the mining power load balancing mechanism can ensure the security of the blockchain by splitting adversaries' mining power. The proposed prototype has been implemented on Amazon EC2. The experiment results show that the proposed protocol achieves the dynamic throughput and reach xxxtpstransactions per second.},
  langid = {english}
}

