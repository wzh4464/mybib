@article{aykanat2004PermutingSparseRectangular,
  title = {Permuting {{Sparse Rectangular Matrices}} into {{Block-Diagonal Form}}},
  author = {Aykanat, Cevdet and Pinar, Ali and {\c C}ataly{\"u}rek, {\"U}mit V.},
