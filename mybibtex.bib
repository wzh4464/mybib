@article{huang2013HumanInteractiveSecure,
  title = {Human Interactive Secure Key and Identity Exchange Protocols in Body Sensor Networks},
  author = {Huang, Xin and Chen, Bangdao and Markham, Andrew and Wang, Qinghua and Yan, Zheng and Roscoe, Andrew William},
  year = {2013},
  journal = {IET Information Security},
  volume = {7},
  number = {1},
  pages = {30--38},
  issn = {1751-8717},
  doi = {10.1049/iet-ifs.2012.0080},
  abstract = {A body sensor network (BSN) is typically a wearable wireless sensor network. Security protection is critical to BSNs, since they collect sensitive personal information. Generally speaking, security protection of BSN relies on identity (ID) and key distribution protocols. Most existing protocols are designed to run in general wireless sensor networks, and are not suitable for BSNs. After carefully examining the characteristics of BSNs, the authors propose human interactive empirical channel-based security protocols, which include an elliptic curve Diffie\textendash Hellman version of symmetric hash commitment before knowledge protocol and an elliptic curve Diffie\textendash Hellman version of hash commitment before knowledge protocol. Using these protocols, dynamically distributing keys and IDs become possible. As opposite to present solutions, these protocols do not need any pre-deployment of keys or secrets. Therefore compromised and expired keys or IDs can be easily changed. These protocols exploit human users as temporary trusted third parties. The authors, thus, show that the human interactive channels can help them to design secure BSNs.},
  langid = {english}
}

@inproceedings{huang2015CommonSelfpolarTriangle,
  type = {Proceedings Paper},
  title = {The {{Common Self-Polar Triangle}} of {{Concentric Circles}} and {{Its Application}} to {{Camera Calibration}}},
  booktitle = {2015 {{IEEE CONFERENCE ON COMPUTER VISION AND PATTERN RECOGNITION}} ({{CVPR}})},
  author = {Huang, Haifei and Zhang, Hui and Cheung, Yiu-ming},
  year = {2015},
  series = {{{IEEE}} Conference on Computer Vision and Pattern Recognition},
  pages = {4065--4072},
  publisher = {{IEEE}},
  address = {{345 E 47TH ST, NEW YORK, NY 10017 USA}},
  issn = {1063-6919},
  abstract = {In projective geometry, the common self-polar triangle has often been used to discuss the position relationship of two planar conics. However, there are few researches on the properties of the common self-polar triangle, especially when the two planar conics are special conics. In this paper, we explore the properties of the common self-polar triangle, when the two conics happen to be concentric circles. We show there exist infinite many common self-polar triangles of two concentric circles, and provide a method to locate the vertices of these triangles. By investigating all these triangles, we find that they encode two important properties. The first one is all triangles share one common vertex, and the opposite side of the common vertex lies on the same line, which are the circle center and the line at the infinity of the support plane. The second is all triangles are right triangles. Based on these two properties, the imaged circle center and the varnishing line of support plane can be recovered simultaneously, and many conjugate pairs on vanishing line can be obtained. These allow to induce good constraints on the image of absolute conic. We evaluate two calibration algorithms, whereby accurate results are achieved. The main contribution of this paper is that we initiate a new perspective to look into circle-based camera calibration problem. We believe that other calibration methods using different circle patterns can benefit from this perspective, especially for the patterns which involve more than two circles.},
  affiliation = {Huang, HF (Corresponding Author), Hong Kong Baptist Univ, Dept Comp Sci, Hong Kong, Hong Kong, Peoples R China. Huang, Haifei; Cheung, Yiu-ming, Hong Kong Baptist Univ, Dept Comp Sci, Hong Kong, Hong Kong, Peoples R China. Huang, Haifei; Zhang, Hui; Cheung, Yiu-ming, BNU HKBU, United Int Coll, Zhuhai, Peoples R China. Zhang, Hui, Shenzhen Key Lab Intelligent Media \& Speech, Shenzhen, Peoples R China.},
  author-email = {mikehuang@uic.edu.hk amyzhang@uic.edu.hk ymc@comp.hkbu.edu.hk},
