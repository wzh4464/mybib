@article{liu2020RealtimePreciseEllipse,
  title = {A {{Real-Time}} and {{Precise Ellipse Detector Via Edge Screening}} and {{Aggregation}}},
  author = {Liu, Zhenyu and Liu, Xia and Duan, Guifang and Tan, Jianrong},
  year = {2020},
