@article{illingworth1988SurveyHoughTransform,
  title = {A {{Survey}} of the {{Hough Transform}}},
  author = {Illingworth, J and Kittler, J},
  year = {1988},
  journal = {Computer Vision, Graphics and Image Processing},
  volume = {44},
  number = {1},
